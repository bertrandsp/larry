{
  "name": "@larry/scraper",
  "version": "1.0.0",
  "description": "Scraping microservice for Larry vocabulary app",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier -w .",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "scraping",
    "vocabulary",
    "larry"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.13",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.3.0",
    "supertest": "^7.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "fast-xml-parser": "^5.2.5",
    "file-type": "^21.0.0",
    "jsdom": "^26.1.0",
    "mammoth": "^1.10.0",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1"
  }
