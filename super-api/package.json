{
  "name": "larry-backend-service",
  "version": "1.0.0",
  "description": "Larry AI-powered vocabulary app backend service",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "dev:workers": "ts-node src/workers/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:workers\"",
    "test": "node test-setup.js",
    "test:enhanced": "node test-enhanced-api.js",
    "test:pipeline": "node test-pipeline.js",
    "test:prompts": "node test-enhanced-prompts.js",
    "test:prompts:simple": "node test-prompts-simple.js",
    "test:tiers": "node test-tier-generation.js",
    "test:quota": "node test-quota-system.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push"
  },
  "keywords": [
    "larry",
    "vocabulary",
    "ai",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.11.0",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "openai": "^5.16.0",
    "prom-client": "^15.1.3",
    "winston": "^3.17.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "concurrently": "^9.2.1",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
