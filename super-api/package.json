{
  "name": "larry-backend-service",
  "version": "1.0.0",
  "description": "Larry AI-powered vocabulary app backend service",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app-supabase.ts",
    "dev:full": "ts-node src/app-full-supabase.ts",
    "dev:workers": "ts-node src/workers/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:workers\"",
    "test": "vitest --run",
    "test:enhanced": "node test-enhanced-api.js",
    "test:pipeline": "node test-pipeline.js",
    "test:prompts": "node test-enhanced-prompts.js",
    "test:prompts:simple": "node test-prompts-simple.js",
    "test:tiers": "node test-tier-generation.js",
    "test:quota": "node test-quota-system.js",
    "test:watch": "vitest",
    "test:optimization": "node test-openai-optimization.js",
    "test:optimization:simple": "node test-simple.js",
    "test:optimization:standalone": "node test-optimization-standalone.js",
    "test:optimization:deploy": "./test-deployment.sh",
    "test:integration": "vitest --run tests/integration/**/*.test.ts"
  },
  "keywords": [
    "larry",
    "vocabulary",
    "ai",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@supabase/supabase-js": "^2.57.4",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.61.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.8.1",
    "openai": "^5.23.2",
    "pg": "^8.16.3",
    "prisma": "^6.16.2",
    "prom-client": "^15.1.3",
    "winston": "^3.18.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
